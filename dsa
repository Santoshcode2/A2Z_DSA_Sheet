#include<iostream>
using namespace std;
int main(){
    int arr[10];
    cout>>"enter the array";
    cin>>arr;
    for(int i=0;i<n;i++){
        cin>>arr[i];
    }
        //selection sort logic
    for(int i=0;i<n-1;i++){
        int min_index=i;
        for(int j=0;j<n;j++){
            if(arr[min_index]>arr[j]){
                  min_index=j;
            }
        }
        swap(arr[min_index],arr[j]);
    }
     return 0;
}